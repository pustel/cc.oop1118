package test;

public class Atom {
	// attributes
	int protonNumber;
	String name;
	String symbol;
	int group;
	int period;
	int oxState;

	// methods
	public void setAttributes(int protonNumber, String name, String symbol, int group, int period, int oxState) {

		this.protonNumber = protonNumber;
		this.name = name;
		this.symbol = symbol;
		this.group = group;
		this.period = period;
		this.oxState = oxState;
	}
	public void setProtonNumber(int protonNumber) {
		this.protonNumber = protonNumber;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setSymbol(String symb) {
		this.symbol = symb;
	}
	public void setGroup(int group) {
		this.group = group;
	}
	public void setPeriod(period) {
		this.period = period;
	}
	public void setOxState(oxState) {
		this.oxState = oxstate;
	}

	//@Override
	//public String toString() {
		// TODO Auto-generated method stub
	//	return "OZ" + protonNumber + " " + name;
	//}

	public Atom() {
		// TODO Auto-generated constructor stub
	}

	public Atom(int protonNumber, String name, String symbol, int group, int period, int oxState) {
		this.protonNumber = protonNumber;
		this.name = name;
		this.symbol = symbol;
		this.group = group;
		this.period = period;
		this.oxState = oxState;
	}

}
